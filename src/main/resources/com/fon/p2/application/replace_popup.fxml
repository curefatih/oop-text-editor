<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="135.0" prefWidth="472.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fon.p2.application.ReplacePopupController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" prefHeight="17.0" prefWidth="60.0" text="Find what:" />
        <TextField fx:id="searchBox" layoutX="91.0" layoutY="10.0" onKeyTyped="#onSearchBoxTextChanged" prefHeight="25.0" prefWidth="131.0" />
        <Button fx:id="findButton" disable="true" layoutX="259.0" layoutY="10.0" mnemonicParsing="false" onAction="#onFindButtonAction" prefHeight="25.0" prefWidth="84.0" text="Find Next" />
      <Label layoutX="14.0" layoutY="42.0" prefHeight="17.0" prefWidth="77.0" text="Replace with:" />
      <TextField fx:id="replaceBox" layoutX="91.0" layoutY="38.0" prefHeight="25.0" prefWidth="131.0" />
      <Button fx:id="replaceButton" disable="false" layoutX="259.0" layoutY="38.0" mnemonicParsing="false" onAction="#onReplaceButtonAction" prefHeight="25.0" prefWidth="84.0" text="Replace" />
      <Button fx:id="replaceButton1" disable="false" layoutX="259.0" layoutY="74.0" mnemonicParsing="false" onAction="#onReplaceAllButtonAction" prefHeight="25.0" prefWidth="84.0" text="Replace All" />
      
    </children>
</AnchorPane>
